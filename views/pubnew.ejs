<%-include("partials/header.ejs")%>
<link rel="stylesheet" href="../stylesheets/signin.css">
    <title>New Publication</title>
    
<div class="container">
<h1>Upload Publication</h1>
<br>
<form action="/uploadpub" method="POST" enctype="multipart/form-data">     
    <div class="form-group">
        <label for="pub">Publication</label>
        <input type="file" class="form-control-file" id="pub" name="pub">
      </div>
    <div class="form-group">
        <label for="caption">Publication Title</label>
        <input type="text" class="form-control" name="title" id="title" placeholder="Example title">
      </div>  
    <button type="submit" class="btn btn-primary" required>Submit</button>
  </form>
</div>
<%-include("partials/footer.ejs")%>